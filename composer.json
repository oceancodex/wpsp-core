{
	"name": "oceancodex/wpsp-core",
	"version": "8.2.0",
	"description": "WordPress Starter Plugin - Core",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress plugin"
	],
	"license": "GPL-3.0-or-later",
	"require": {
		"php": "^8.2",
		"ext-curl": "*",
		"ext-intl": "*",
		"ext-json": "*",
		"dflydev/dot-access-data": "*",
		"doctrine/doctrine-migrations-bundle": "*",
		"doctrine/orm": "*",
		"fakerphp/faker": "*",
		"gedmo/doctrine-extensions": "*",
		"hassankhan/config": "*",
		"illuminate/database": "*",
		"illuminate/translation": "*",
		"illuminate/view": "*",
		"ocramius/generated-hydrator": "*",
		"symfony/http-client": "*",
		"vlucas/phpdotenv": "*",
		"yahnis-elsts/plugin-update-checker": "*"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"WPSPCORE\\": "src/"
		},
		"files": [
			"src/Helpers/Helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"WPSPCORE\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "OceanCodex",
			"email": "contact@oceancodex.com"
		}
	],
	"scripts": {
		"post-autoload-dump": [
		],
		"post-root-package-install": [
		],
		"post-create-project-cmd": [
		]
	},
	"require-dev": {
	}
}
