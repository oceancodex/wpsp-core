{
	"name": "oceancodex/wpsp-core",
	"version": "1.0.0",
	"description": "WordPress Starter Plugin - Core",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress plugin"
	],
	"license": "GPL-3.0-or-later",
	"require": {
		"php": "^8.2",
		"ext-curl": "*",
		"ext-intl": "*",
		"ext-json": "*",
		"dflydev/dot-access-data": "^3.0",
		"doctrine/doctrine-migrations-bundle": "^3.3",
		"doctrine/orm": "^3.2",
		"fakerphp/faker": "^v1.23.1",
		"gedmo/doctrine-extensions": "^3.14",
		"hassankhan/config": "^3.1",
		"illuminate/database": "^11.10",
		"illuminate/translation": "^11.10",
		"illuminate/view": "^11.10",
		"ocramius/generated-hydrator": "^4.6",
		"symfony/http-client": "^7.1",
		"vlucas/phpdotenv": "^5.6",
		"yahnis-elsts/plugin-update-checker": "^5.4"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"WPSPCORE\\": "src/"
		},
		"files": [
			"src/Helpers/Helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"WPSPCORE\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "OceanCodex",
			"email": "contact@oceancodex.com"
		}
	],
	"scripts": {
		"post-autoload-dump": [
		],
		"post-root-package-install": [
		],
		"post-create-project-cmd": [
		]
	},
	"require-dev": {
	}
}
