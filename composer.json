{
	"name": "oceancodex/wpsp-core",
	"version": "8.2.0",
	"description": "WordPress Starter Plugin - Core",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress plugin"
	],
	"license": "GPL-3.0-or-later",
	"require": {
		"php": "^8.2",
		"ext-ctype": "*",
		"ext-json": "*",
		"ext-pcre": "*",
		"ext-pdo": "*",
		"ext-mbstring": "*",
		"ext-tokenizer": "*",
		"dflydev/dot-access-data": "^v3.0.3",
		"doctrine/doctrine-migrations-bundle": "^3.3.1",
		"doctrine/orm": "^3.2.1",
		"fakerphp/faker": "^v1.23.1",
		"gedmo/doctrine-extensions": "^v3.16.1",
		"hassankhan/config": "^3.1.0",
		"illuminate/database": "^v11.16.0",
		"illuminate/translation": "^v11.16.0",
		"illuminate/view": "^v11.16.0",
		"ocramius/generated-hydrator": "^4.6.0",
		"symfony/http-client": "^v7.1.2",
		"symfony/rate-limiter": "^v7.1.1",
		"vlucas/phpdotenv": "^v5.6.0",
		"yahnis-elsts/plugin-update-checker": "^v5.4"
	},
	"require-dev": {
		"spatie/ignition": "^1.15.0"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"WPSPCORE\\": "src/"
		},
		"files": [
			"src/Helpers/Helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"WPSPCORE\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "OceanCodex",
			"email": "contact@oceancodex.com"
		}
	],
	"scripts": {
		"post-autoload-dump": [
		],
		"post-root-package-install": [
		],
		"post-create-project-cmd": [
		]
	}
}
