{
	"name": "oceancodex/wpsp-core",
	"version": "8.0.0",
	"description": "WordPress Starter Plugin - Core",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress plugin"
	],
	"license": "GPL-3.0-or-later",
	"require": {
		"php": "^8.0",
		"ext-curl": "*",
		"ext-intl": "*",
		"ext-json": "*",
		"dflydev/dot-access-data": "^3.0",
		"doctrine/doctrine-migrations-bundle": "^3.3",
		"doctrine/orm": "^2.19.6",
		"fakerphp/faker": "^1.23",
		"gedmo/doctrine-extensions": "^3.16",
		"hassankhan/config": "^3.1",
		"illuminate/database": "^v9.52.16",
		"illuminate/translation": "^v9.52.16",
		"illuminate/view": "^v9.52.16",
		"mongodb/laravel-mongodb": "^v3.9.5",
		"ocramius/generated-hydrator": "^4.4.0",
		"symfony/http-client": "^v6.0.20",
		"symfony/rate-limiter": "^v6.0.19",
		"vlucas/phpdotenv": "^5.6",
		"yahnis-elsts/plugin-update-checker": "^5.4"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"WPSPCORE\\": "src/"
		},
		"files": [
			"src/Helpers/Helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"WPSPCORE\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "OceanCodex",
			"email": "contact@oceancodex.com"
		}
	],
	"scripts": {
		"post-autoload-dump": [
		],
		"post-root-package-install": [
		],
		"post-create-project-cmd": [
		]
	}
}
